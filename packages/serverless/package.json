  "license": "MIT",
  "scripts": {
    "clean": "rimraf .build .output dist",
    "build:tsc": "yarn clean && tsc --project tsconfig.prod.json",
    "build": "yarn build:tsc & sls package --package .output/lambdas && rimraf .output/**/*.json",
    "start": "yarn build:tsc && NODE_TLS_REJECT_UNAUTHORIZED=0 sls offline start --host 0.0.0.0"
  },
